    <style name="Babylon.Theme" parent="@android:style/Theme.Black.NoTitleBar" />

    <style name="Default.Theme" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:spinnerStyle">@style/spinner</item>
        <item name="android:editTextStyle">@style/edit_text</item>
        <item name="android:radioButtonStyle">@style/radio_button</item>
        <item name="android:checkboxStyle">@style/checkBox</item>
        <item name="android:buttonStyle">@style/button</item>
        <item name="android:listViewStyle">@style/ListViewAppTheme</item>
        <item name="android:listViewWhiteStyle">@style/ListViewAppTheme.White</item>
        <item name="android:spinnerItemStyle">@style/SpinnerItemAppTheme</item>
    </style>

    <style name="HomeButton">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textSize">@dimen/text_size_small</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@null</item>
    </style>

    <style name="ListViewAppTheme" parent="android:Widget.ListView">
        <item name="android:listSelector">@drawable/list_selector_holo_light</item>
    </style>

    <style name="ListViewAppTheme.White" parent="android:Widget.ListView.White">
        <item name="android:listSelector">@drawable/list_selector_holo_light</item>
    </style>

    <style name="MainScreen.Theme" parent="@android:style/Theme.Dialog">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:windowNoTitle">true</item>
        <!-- <item name="android:windowBackground">@color/black_transparence</item> -->
        <item name="android:windowBackground">@drawable/main_activity_gradient</item>
        <item name="android:spinnerStyle">@style/spinner</item>
        <item name="android:gravity">center</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="SpinnerItemAppTheme" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="ToastScreen.Theme" parent="@android:style/Theme.Dialog">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Toast</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="actionBarTitleTextStyle">
        <item name="android:textColor">@color/color_666666</item>
        <item name="android:textSize">@dimen/text_size_30pt</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="button" parent="android:Widget.Button">
        <item name="android:background">@drawable/button_blue_selector</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/text_size_20pt</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="checkBox" parent="android:Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/check_box</item>
        <item name="android:textColor">@color/color_666666</item>
        <!-- <item name="android:childIndicatorLeft">20dp</item> -->
    </style>

    <style name="dialogTextStyle">
        <item name="android:textColor">@color/color_666666</item>
        <item name="android:textSize">@dimen/text_size_20pt</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingBottom">20dp</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="dialogTitleStyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size_24pt</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="edit_text" parent="android:Widget.EditText">
        <item name="android:background">@drawable/edit_text</item>
        <item name="android:textColor">@color/color_666666</item>
    </style>

    <style name="infoItemStyle">
        <item name="android:textColor">@color/color_666666</item>
        <item name="android:textSize">@dimen/text_size_24pt</item>
        <item name="android:padding">15dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="instructionImageStyle">
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="instructionPageStyle">
        <item name="android:orientation">vertical</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="instructionTextBoldStyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size_20pt</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="instructionTextStyle">
        <item name="android:textColor">@color/color_666666</item>
        <item name="android:textSize">@dimen/text_size_24pt</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="radio_button" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/radio_button_selector</item>
        <item name="android:textColor">@color/color_666666</item>
    </style>

    <style name="settingSectionStyle">
        <item name="android:textColor">@color/blue</item>
        <item name="android:textSize">@dimen/text_size_34pt</item>
        <item name="android:padding">10dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="settingSubSectionStyle">
        <item name="android:textColor">@color/color_666666</item>
        <item name="android:textSize">@dimen/text_size_21pt</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:background">@drawable/divider</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="spinner" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/spinner_background</item>
        <item name="android:textColor">@color/color_33b5e5</item>
        <item name="android:spinnerMode">dropdown</item>
    </style>

